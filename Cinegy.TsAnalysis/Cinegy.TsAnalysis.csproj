<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Version>0.0.0-beta</Version>
    <Authors>Lewis Kirkaldie</Authors>
    <Company>Cinegy GmbH</Company>
    <Description>Analysis engine for parsing data from Cinegy TsDecoder, in C# targetting NET Standard 2.0</Description>
    <Copyright>Cinegy GmbH</Copyright>
    <PackageTags>Transport Stream Analysis MPEGTS</PackageTags>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Cinegy/TsAnalysis</PackageProjectUrl>
    <PackageReleaseNotes>Moved up to netstandard2.0 and dependency update </PackageReleaseNotes>
    <NeutralLanguage />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cinegy.Telemetry" Version="2.0.20-beta" />
    <PackageReference Include="Cinegy.TtxDecoder" Version="2.0.37-beta" />
    <PackageReference Include="Cinegy.TsDecoder" Version="3.0.113-beta" />
  </ItemGroup>


</Project>
